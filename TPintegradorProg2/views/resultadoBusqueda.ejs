<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>FakeBook</title>
    <link rel="icon"
        href="https://w7.pngwing.com/pngs/624/811/png-transparent-computer-icons-facebook-social-media-f-logo-cross-brand.png"
        type="image/x-icon">

    <!-- <link rel="stylesheet" href="css/resultadoBusqueda.css"> -->

    <link rel="stylesheet" href="/stylesheets/general.css">

    <!-- UiKit -->
    <!-- <link rel="stylesheet" href="css/uikit.min.css">
    <script src="js/uikit-icons.min.js"></script>
    <script src="js/uikit.min.js"></script> -->
    <!-- UiKit -->

    <!-- UIkit CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/uikit@3.5.5/dist/css/uikit.min.css" />

    <!-- UIkit JS -->
    <script src="https://cdn.jsdelivr.net/npm/uikit@3.5.5/dist/js/uikit.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/uikit@3.5.5/dist/js/uikit-icons.min.js"></script>
</head>

<body>
    

    <%- include('./partials/headerLogueado') %> 


    <main>
        <div class="uk-section uk-section-small">
            <div class="uk-container uk-container-large uk-flex uk-flex-middle uk-flex-center">
               
                <div class="listadoUsuario">
                <% if (usuarios.length) { %> 
                    <h1> Listado de usuarios </h1>
                    
                <% for(let i  = 0; i < usuarios.length; i++){ %> 
                    <li>
                        <a href="/usuario/detalle/<%= usuarios[i].id %>"> <%= usuarios[i].username %> </a>
                    </li>
                <% } } else { %> 
                    <h2>No se encontraron usuarios</h2>
                <% } %> 
                </div>
                <div class="listadoPosteo">
                <% if (posteos.length) { %> 
                    <h1> Listado de posteos </h1>
                <% for(let i  = 0; i < posteos.length; i++){ %> 
                    <li>
                        <a href="posts/detalle/<%= posteos[i].id %>"> <%= posteos[i].titulo %> </a>
                    </li>
                <% } } else { %> 
                    <h2>No se encontraron posteos</h2>
                <% } %> 
                </div>
            </div>
        </div>
    </main>

    <%- include('./partials/footer') %> 


</body>

</html>